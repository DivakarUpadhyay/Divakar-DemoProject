<section class="section git_fullwidth inv_sub_update">
    <img src="~/Content/images/bg-inv-sub-update.jpg" alt="Subscribe to Investor Updates" class="bg-Img">
    <div class="container">
        <div class="ask_expert">
            <div class="ae_form_hld">
                <h2 class="left_line">Subscribe to Investor Updates</h2>
                <div class="wp_controlers">
                    <div class="wp_control width_half">
                        <input type="text" placeholder="First Name*" class="textBox" />
                    </div>
                    <div class="wp_control width_half">
                        <input type="text" placeholder="Last Name*" class="textBox" />
                    </div>
                    <div class="wp_control width_half">
                        <input type="text" placeholder="Email ID*" class="textBox" />
                    </div>
                    <div class="wp_control width_half">
                        <input type="text" placeholder="Job Title" class="textBox" />
                    </div>
                    <div class="wp_control width_half">
                        <input type="text" placeholder="Company" class="textBox" />
                    </div>
                    <div class="wp_control width_half">
                        <select name="" id="" class="selectBox">
                            <option value="">Country</option>
                            <option value="">India</option>
                        </select>
                    </div>
                    <div class="wp_control">
                        <textarea placeholder="Message" class="textArea"></textarea>
                    </div>
                </div>
                <div class="button"><span>Subscribe</span></div>
            </div>
        </div>
    </div>
</section>

