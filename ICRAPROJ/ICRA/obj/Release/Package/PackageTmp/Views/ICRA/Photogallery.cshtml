
@{
    ViewBag.Title = "Photogallery";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="~/Scripts/ICRA_JS/media-min.js"></script>
<section id="banner" class="inner_banner">
    <div class="banner_item">
        <picture>
            <source media="(max-width:767px)" srcset="~/Content/images/banner-photo-gallery-mob-01.jpg" />
            <img src="~/Content/images/banner-photo-gallery-01.jpg" alt="" />
        </picture>
        <div class="container">
            <div class="banner_det">
                <h1>Photo Gallery</h1>
                <p>Explore all the photos of icra ratings' clients,<br> employee, stakeholders and communities</p>
            </div>
        </div>
    </div>
</section>

<div class="breadcrumbs">
    <div class="container">
        <a href="index.html">Home</a>Photo Gallery
    </div>
</div>

<section class="section photo_gallery_filter">
    <div class="container">
        <div class="rating_filter_box_hld" style="display:block;">
            <div class="rating_filter_box">
                <div class="wp_control">
                    <select class="selectBox">
                        <option>Year</option>
                        <option>2021</option>
                        <option>2020</option>
                    </select>
                </div>
                <div class="button"><span>Search</span></div>
            </div>
        </div>
    </div>
</section>

<section class="section gallery_latest_event gray_bg">
    <div class="container">
        <div class="gallery_event_wrapper">
            <div class="row align-items-center">
                <div class="col-md-6">
                    <div class="gallery_event_slider" id="latestGallery">
                        <div class="item">
                            <div class="imgGal_list">
                                <a href="~/Content/images/gallery/event-gallery-1.jpg" class="image">
                                    <div class="thumb">
                                        <img src="~/Content/images/gallery/event-gallery-1.jpg" alt="">
                                    </div>
                                </a>
                            </div>
                        </div>
                        <div class="item">
                            <div class="imgGal_list">
                                <a href="~/Content/images/gallery/event-gallery-1.jpg" class="image">
                                    <div class="thumb">
                                        <img src="~/Content/images/gallery/event-gallery-1.jpg" alt="">
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="gal-event-text">
                        <h6>ICRA's Chennai team during the Conference on Indian Auto Component Industry</h6>
                        <p><span class="date">Feb 2019</span>(L-R) Mr. R. Raghuttama Rao (Moderator), Mr. Arvind Balaji, Mr. Harish Lakshman, Mr. A.S. Raju, Dr. Seshu Bhagavathula, Mr. S.G. Mani, Mr. Subrata Ray in an intriguing panel discussion during Conference on 'Changing Landscape in the Indian Auto Component Industry' in Chennai.</p>
                        <a href="#" class="rv_more">View Images <i class="fas fa-arrow-right"></i></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

