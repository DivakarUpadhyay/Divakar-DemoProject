                </div>
            </div>
        </div>
    </section>

    <div class="breadcrumbs">
        <div class="container">
            <a href="@Url.Action("Index","ICRA")">Home</a><a href="@Url.Action("Research", "ICRA")">Research Services </a>@Model.research_Section1.title1
