                                imageName = bannerObj.ReadImage(item.Icon);
                                <div class="sector_type_blurb" id="btnCategoryId" onclick="return PopUpDrop(@item.Research_Category_Id);">
                                    <img src="@imageName" alt="@item.Research_Category_Name">
                                    <span>@item.Research_Category_Name</span>
                                </div>
                            }

                        }



                    </div>
                </div>
            }

            <div class="search_hld">
                <h2 class="left_line">Find Reports</h2>
                <div class="search_con">
                    <input type="text" id="txtSearchReportName" placeholder="Search Report Name" class="textBox" />
                    <i class="fas fa-search icon_search" id="btnSearchText"></i>
                    <i class="fas fa-times icon_close"></i>
                </div>
            </div>

            <div class="rating_filter_box_hld">
                <div class="rating_filter_box">
                    <div class="wp_control">
                        @Html.DropDownList("ListItemsCat", Model.ListItemsCat, "Category", new { @class = "selectBox" })
                    </div>
                    <div class="wp_control">
                        @Html.DropDownList("ListItemsSector", Model.ListItemsSector, "Sector", new { @class = "selectBox" })
                    </div>
                    <div class="wp_control">
