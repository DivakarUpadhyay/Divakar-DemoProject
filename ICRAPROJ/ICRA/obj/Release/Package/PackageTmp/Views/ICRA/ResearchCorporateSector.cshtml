        function alphaOnly(evt) {
            var charCode = (evt.which) ? evt.which : window.event.keyCode;

            if (charCode <= 13) {
                return true;
            }
            else {
                var keyChar = String.fromCharCode(charCode);
                var re = /^[a-zA-Z- ]+(\s+[a-zA-Z]+)*$/;
                return re.test(keyChar);
            }
        }
        function isNumberOnly(evt) {
            var charCode = (evt.which) ? evt.which : window.event.keyCode;

            if (charCode <= 13) {
                return true;
            }
            else {
                var keyChar = String.fromCharCode(charCode);
                var re = /^\d+$/;
                return re.test(keyChar);
            }
        }

