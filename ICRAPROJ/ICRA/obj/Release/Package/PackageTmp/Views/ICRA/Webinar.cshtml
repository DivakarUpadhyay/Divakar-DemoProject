<section id="banner" class="inner_banner">
    <div class="banner_item">
        <picture>
            <source media="(max-width:767px)" srcset="~/Content/images/banner-webinar-mob.jpg" />
            <img src="~/Content/images/banner-webinar.jpg" alt="Webinar" />
        </picture>
        <div class="container">
            <div class="banner_det">
                <h1>Webinar</h1>
            </div>
        </div>
    </div>
</section>

<div class="breadcrumbs">
    <div class="container">
        <a href="@Url.Action("Index","ICRA")">Home</a><a href="@Url.Action("Mediaandevents","ICRA")">Media</a>Webinar
