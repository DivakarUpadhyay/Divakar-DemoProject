@model string

@{
    string classname = "get_in_touch gray_bg";

    @*For Full Width Background Image Start*@
    if (Model != null)
    {
        if (Model == "Research")
        {
            classname = "git_fullwidth";
        }
    }
    @*For Full Width Background Image End*@
    <section class="section @classname">

        @*For Full Width Background Image*@
        @if (Model != null)
        {
            if (Model == "Research")
            {
                <img src="~/Content/images/banner-ask-expert.jpg" alt="" class="bg-Img">
            }
        }
        <div class="container">
            <div class="ask_expert">
                @*For 90% Width Background Image*@
                @if (Model != null)
                {
                    if (Model == "Rating" || Model== "researchcorporatesector" || Model== "Researchdetail" || Model== "Allresearchreports" || Model== "RatingCorporate")
                    {
                        <img src="~/Content/images/banner-ask-expert.jpg" alt="" class="bg-Img">
                    }
                }
                <div class="ae_form_hld">
                    <div id="tabdata_02">
                        <div class="switch_tab_hld">
                            <div class="slider"></div>
                            <div class="tab active" tabId="industry_analyst">Ask Our Industry Analyst</div>
                            <div class="tab" tabId="buss_representative">Get in touch with our Business Representative</div>
                        </div>
                        <div class="errMsg" id="msgResponse">You can show error msg here</div>
                        <div class="switch_det_panel show" id="tab_industry_analyst">
                            @if (Model != null)
                            {
                                <input type="hidden" id="PageName" value="@Model" />
                            }

                            <div class="wp_controlers">
                                <div class="wp_control width_half">
                                    <input type="text" id="txtNameAskOurIndustry" placeholder="Name*" maxlength="75" onkeypress="return alphaOnly(this);" class="textBox">
                                    <div class="errMsg" id="lblNameAskOurIndustry" style="display:none"></div>
                                </div>
                                <div class="wp_control width_half">
                                    <input type="text" id="txtCompanyNameAskOurIndustry" placeholder="Company Name*" maxlength="150" class="textBox">
                                    <div class="errMsg" id="lblCompanyNameAskOurIndustry" style="display:none"></div>
                                </div>
                                <div class="wp_control width_half">
                                    <input type="text" id="txtEmailIdAskOurIndustry" placeholder="Email ID*" class="textBox">
                                    <div class="errMsg" id="lblEmailIdAskOurIndustry" style="display:none"></div>
                                </div>
                                <div class="wp_control width_half">
                                    <input type="text" id="txtContactNumberAskOurIndustry" maxlength="15" onkeypress="return isNumberOnly(this);" minlength="10" placeholder="Contact Number*" class="textBox">
                                    <div class="errMsg" id="lblContactNumberAskOurIndustry" style="display:none"></div>

                                </div>
                                <div class="wp_control">
                                    <input type="text" id="txtWriteQueryAskOurIndustry" placeholder="Write Your Query*" maxlength="250" class="textBox">
                                    <div class="errMsg" id="lblWriteQueryAskOurIndustry" style="display:none"></div>
                                </div>
                                <div class="note">(*Denotes compulsory field)</div>
                            </div>
                            <div class="button" id="btnSubmitindustry" onclick="return EmailInsert('askIndustry');"><span>Submit</span></div>

                        </div>

                        <div class="switch_det_panel" id="tab_buss_representative">
                            <div class="wp_controlers">
                                <div class="wp_control width_half">
                                    <input type="text" id="txtNameBusinessRepresentative" maxlength="75" onkeypress="return alphaOnly(this);" placeholder="Name*" class="textBox">
                                    <div class="errMsg" id="lblNameBusinessRepresentative" style="display:none"></div>
                                </div>
                                <div class="wp_control width_half">
                                    <input type="text" id="txtCompanyNameBusinessRepresentative" placeholder="Company Name*" maxlength="150" class="textBox">
                                    <div class="errMsg" id="lblCompanyNameBusinessRepresentative" style="display:none"></div>
                                </div>
                                <div class="wp_control width_half">
                                    <input type="text" id="txtEmailIDBusinessRepresentative" placeholder="Email ID*" class="textBox">
                                    <div class="errMsg" id="lblEmailIDBusinessRepresentative" style="display:none"></div>
                                </div>
                                <div class="wp_control width_half">
                                    <input type="text" id="txtContactNumberBusinessRepresentative" placeholder="Contact Number*" maxlength="15" onkeypress="return isNumberOnly(this);" minlength="10" class="textBox">
                                    <div class="errMsg" id="lblContactNumberBusinessRepresentative" style="display:none"></div>
                                </div>
                                <div class="wp_control">
                                    <input type="text" id="txtWriteQueryBusinessRepresentative" placeholder="Write Your Query*" maxlength="250" class="textBox">
                                    <div class="errMsg" id="lblWriteQueryBusinessRepresentative" style="display:none"></div>
                                </div>
                                <div class="note">(*Denotes compulsory field)</div>
                            </div>
                            <div class="button" id="btnSubmitbusinessRepresentative" onclick="return EmailInsert('BusinessRepresentative');"><span>Submit</span></div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
}
