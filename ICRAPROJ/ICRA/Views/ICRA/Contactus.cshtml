            }
            console.log(data)
            var json = JSON.stringify(data);
            console.log(json)
            $.ajax({
                type: "POST",
                url: "/ICRA/InsertContactUs_Email",
                data: json,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    if (response.Status == 200) {
                        alert("Data Saved Successfully")
