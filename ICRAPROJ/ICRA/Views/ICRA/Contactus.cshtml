        function EmailInsert(section)
        {
            if (section == 'callback') {
                if ($("#txtNamecallback").val() == "") {
                    $("#Nameerrocallback").css('display', 'block');
                    $("#Nameerrocallback").text("Please Enter Name")
                    $("#txtNamecallback").focus();
                    return false;
                } else {

                    $("#Nameerrocallback").css('display', 'none');
                }
                if ($("#txtEmailIdcallback").val() == "") {
