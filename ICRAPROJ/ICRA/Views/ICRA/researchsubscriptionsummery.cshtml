<section id="banner" class="inner_banner inner_banner_ver_2">
    <div class="banner_item">
        <picture>
            <source media="(max-width:767px)" srcset="~/Content/images/banner-research-corporate-sector-mob.jpg" />
            <img src="~/Content/images/banner-research-corporate-sector.jpg" alt="Credit Perspective Reports" />
        </picture>
        <div class="container">
            <div class="banner_det investor_banner">
                <h1>Research Subscription</h1>
